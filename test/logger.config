[loggers]
keys=root,rai

[handlers]
keys=stream_handler

[formatters]
keys=fmt

[logger_root]
level=INFO
handlers=stream_handler

[logger_rai]
level=DEBUG
handlers=stream_handler
qualname=rai

[handler_stream_handler]
class=StreamHandler
level=DEBUG
formatter=fmt
args=(sys.stdout,)

[formatter_fmt]
format=%(asctime)s - %(name)s - %(levelname)s - %(message)s
